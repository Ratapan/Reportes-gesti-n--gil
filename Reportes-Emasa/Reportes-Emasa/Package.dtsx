            string from = "gestionagilreportes@outlook.com";
            string to = "ja.sabando@duocuc.cl";
            string asunto = "Reporte diario Owners";
            string path = @"C:\Reportes-gestion-agil\Reportes\send_report.xls";
            string body = $@"
            <body>
            <h2>Buen día,</h2>
            <p>Este es tu reporte diario de cantidad de ventas por owner, <i>tambien se añadio 'types', para indicar los diferentes tipos de productos</i> </p>
