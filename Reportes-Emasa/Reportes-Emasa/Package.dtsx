            SendMailMessage(from, to, asunto, body, path);
            Dts.TaskResult = (int)ScriptResults.Success;

        }
        private void SendMailMessage(string From, string SendTo, string Subject, string Body, string Path)
        {
            Console.WriteLine("send email");

            MailMessage msg = new MailMessage();
            msg.From = new MailAddress(From);
            msg.To.Add(SendTo);
            msg.To.Add("jaimeantonioadd@gmail.com");
            msg.Subject = Subject;
            msg.Body = Body;
            msg.Attachments.Add(new Attachment(Path));
            msg.IsBodyHtml = true;
            Console.WriteLine("msg ok");
            SmtpClient client = new SmtpClient("smtp.office365.com", 587);
            Console.WriteLine("client ok");
            client.Credentials = new NetworkCredential(From, "scrum2022");
            Console.WriteLine("credential ok");
            client.EnableSsl = true;
            Console.WriteLine("all ok");
            client.Send(msg);

        }

